# Auto detect text files and perform LF normalization
* text=auto
*.pdf filter=xet diff=xet merge=xet -text
*.jpg filter=xet diff=xet merge=xet -text
*.jpeg filter=xet diff=xet merge=xet -text
*.png filter=xet diff=xet merge=xet -text
*.ttf filter=xet diff=xet merge=xet -text
*.joblib filter=xet diff=xet merge=xet -text
*.db filter=xet diff=xet merge=xet -text
*.pdf filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.ttf filter=lfs diff=lfs merge=lfs -text
*.joblib filter=lfs diff=lfs merge=lfs -text
*.db filter=lfs diff=lfs merge=lfs -text
